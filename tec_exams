        // Helper function to transform the raw JSON structure into the desired format
        function transformData(rawData, questionKey, answerKey) {
            // Find the index of the header row and skip it
            const headerRowIndex = rawData.findIndex(item => item[questionKey] === "Question");
            const dataRows = headerRowIndex !== -1 ? rawData.slice(headerRowIndex + 1) : rawData;

            return dataRows.map(item => ({
                // Ensure the question number is converted to a string for consistency
                "Question": String(item[questionKey]),
                // 'Column3' is the key for the answers in the provided files
                "Answer": item[answerKey]
            }));
        }

