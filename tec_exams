Â  Â  Â  Â  const selector = document.getElementById("exam-selector");
Â  Â  Â  Â  const tbody = document.querySelector("#qaTable tbody");

Â  Â  Â  Â  /**
Â  Â  Â  Â  Â * Renders the data for a specific exam key into the table.
Â  Â  Â  Â  Â * @param {string} examKey - The key of the exam data in EXAM_DATA.
Â  Â  Â  Â  Â */
Â  Â  Â  Â  function renderTable(examKey) {
Â  Â  Â  Â  Â  Â  // Get the data for the selected exam
Â  Â  Â  Â  Â  Â  const dataToDisplay = EXAM_DATA[examKey];

Â  Â  Â  Â  Â  Â  // Clear the current table body
Â  Â  Â  Â  Â  Â  tbody.innerHTML = "";

Â  Â  Â  Â  Â  Â  if (!dataToDisplay) {
Â  Â  Â  Â  Â  Â  Â  Â  tbody.innerHTML = '<tr><td colspan="2" style="text-align: center;">No data found for this exam.</td></tr>';
Â  Â  Â  Â  Â  Â  Â  Â  return;
Â  Â  Â  Â  Â  Â  }

Â  Â  Â  Â  Â  Â  // Iterate through the data and create table rows
Â  Â  Â  Â  Â  Â  dataToDisplay.forEach(item => {
Â  Â  Â  Â  Â  Â  Â  Â  const row = document.createElement("tr");
Â  Â  Â  Â  Â  Â  Â  Â  // The keys 'Question' and 'Answer' are based on the clean structure above
Â  Â  Â  Â  Â  Â  Â  Â  row.innerHTML = `<td>${item.Question}</td><td>${item.Answer}</td>`;
Â  Â  Â  Â  Â  Â  Â  Â  tbody.appendChild(row);
Â  Â  Â  Â  Â  Â  });
Â  Â  Â  Â  }

Â  Â  Â  Â  /**
Â  Â  Â  Â  Â * Initializes the dropdown with available exam names and sets up the change listener.
Â  Â  Â  Â  Â */
Â  Â  Â  Â  function initializeSelector() {
Â  Â  Â  Â  Â  Â  // Get all exam names (keys) from the EXAM_DATA object
Â  Â  Â  Â  Â  Â  const examNames = Object.keys(EXAM_DATA);

Â  Â  Â  Â  Â  Â  // Populate the dropdown
Â  Â  Â  Â  Â  Â  examNames.forEach(name => {
Â  Â  Â  Â  Â  Â  Â  Â  const option = document.createElement("option");
Â  Â  Â  Â  Â  Â  Â  Â  option.value = name;
Â  Â  Â  Â  Â  Â  Â  Â  option.textContent = name;
Â  Â  Â  Â  Â  Â  Â  Â  selector.appendChild(option);
Â  Â  Â  Â  Â  Â  });

Â  Â  Â  Â  Â  Â  // Add an event listener to re-render the table when a new exam is selected
Â  Â  Â  Â  Â  Â  selector.addEventListener('change', (event) => {
Â  Â  Â  Â  Â  Â  Â  Â  renderTable(event.target.value);
Â  Â  Â  Â  Â  Â  });

Â  Â  Â  Â  Â  Â  // Load the first exam by default
Â  Â  Â  Â  Â  Â  if (examNames.length > 0) {
Â  Â  Â  Â  Â  Â  Â  Â  renderTable(examNames[0]);
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  }

Â  Â  Â  Â  // Run the initialization function when the script loads
Â  Â  Â  Â  initializeSelector();
Â  Â  </script>
